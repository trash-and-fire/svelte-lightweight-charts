{
  "name": "svelte-lightweight-charts",
  "version": "1.0.0",
  "description": "Svelte wrapper for financial lightweight charts built with HTML5 canvas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/trash-and-fire/svelte-lightweight-charts.git"
  },
  "bugs": {
    "url": "https://github.com/trash-and-fire/svelte-lightweight-charts/issues"
  },
  "keywords": [
    "svelte",
    "lightweight-charts",
    "financial-charting-library",
    "charting-library",
    "html5-charts",
    "canvas",
    "typescript",
    "charting",
    "charts"
  ],
  "main": "dist/index.js",
  "files": [
    "dist/**"
  ],
  "devDependencies": {
    "@tsconfig/svelte": "1.0.10",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.15.1",
    "cross-env": "7.0.3",
    "css-loader": "5.0.1",
    "eslint": "7.20.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-svelte3": "3.1.1",
    "fork-ts-checker-webpack-plugin": "6.1.1",
    "gulp": "4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-rename": "2.0.0",
    "gulp-transform": "3.0.5",
    "html-webpack-plugin": "5.3.1",
    "husky": "4.3.8",
    "lightweight-charts": "3.3.0",
    "lint-staged": "10.5.4",
    "lodash": "4.17.21",
    "mini-css-extract-plugin": "1.3.4",
    "plugin-error": "1.0.1",
    "svelte": "3.31.2",
    "svelte-check": "1.3.0",
    "svelte-loader": "3.0.0",
    "svelte-preprocess": "4.6.9",
    "ts-loader": "8.0.4",
    "tslib": "2.0.1",
    "typescript": "4.0.3",
    "webpack": "5.23.0",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "3.11.2"
  },
  "scripts": {
    "prepublishOnly": "npm run build-package",
    "build-package": "tsc --project tsconfig.build.json && gulp build",
    "build-demo": "npm run build-package && cross-env NODE_ENV=production webpack --bail",
    "start": "npm run build-package && webpack serve --content-base src/demo/public",
    "validate": "svelte-check"
  },
  "peerDependencies": {
    "lightweight-charts": ">=3.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "svelte-check && lint-staged"
    }
  },
  "lint-staged": {
    "*.js|*.ts|*.svelte": "eslint --cache --fix"
  },
  "engines": {
    "node": ">=10.3.0"
  }
}
